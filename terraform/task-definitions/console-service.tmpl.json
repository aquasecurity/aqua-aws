[
    {
        "dnsSearchDomains": null,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${awslogs_group}",
                "awslogs-region": "${awslogs_region}",
                "awslogs-stream-prefix": "ecs"
            }
        },
        "entryPoint": null,
        "portMappings": [
            {
                "hostPort": ${aqua_server_console_port},
                "protocol": "tcp",
                "containerPort": ${aqua_server_console_port}
            },
            {
                "hostPort": ${aqua_server_gateway_port},
                "protocol": "tcp",
                "containerPort": ${aqua_server_gateway_port}
            }
        ],
        "command": null,
        "linuxParameters": null,
        "cpu": 512,
        "environment": [   
            {
                "name": "AQUA_GRPC_MODE",
                "value": "1"
            },
            {
                "name": "SCALOCK_AUDIT_DBHOST",
                "value": "${db_hostname}"
            },
            {
                "name": "SCALOCK_AUDIT_DBNAME",
                "value": "slk_audit"
            },
            {
                "name": "SCALOCK_AUDIT_DBPORT",
                "value": "${db_port}"
            },
            {
                "name": "SCALOCK_AUDIT_DBUSER",
                "value": "${db_username}"
            },
            {
                "name": "SCALOCK_DBHOST",
                "value": "${db_hostname}"
            },
            {
                "name": "SCALOCK_DBNAME",
                "value": "scalock"
            },
            {
                "name": "SCALOCK_DBPORT",
                "value": "${db_port}"
            },
            {
                "name": "SCALOCK_DBUSER",
                "value": "${db_username}"
            }
        ],
        "resourceRequirements": null,
        "ulimits": null,
        "repositoryCredentials": {
            "credentialsParameter": "${credentialsParameter}"
        },
        "dnsServers": null,
        "mountPoints": [
            {
                "readOnly": null,
                "containerPath": "/var/run/docker.sock",
                "sourceVolume": "docker-socket"
            }
        ],
        "workingDirectory": null,
        "secrets": [
            {
                "name": "ADMIN_PASSWORD",
                "valueFrom": "/aws/reference/secretsmanager/${admin_password}"
            },
            {
                "name": "SCALOCK_DBPASSWORD",
                "valueFrom": "/aws/reference/secretsmanager/${db_password}"
            },
            {
                "name": "SCALOCK_AUDIT_DBPASSWORD",
                "valueFrom": "/aws/reference/secretsmanager/${db_password}"
            },
            {
                "name": "LICENSE_TOKEN",
                "valueFrom": "/aws/reference/secretsmanager/${license_token}"
            }
        ],
        "dockerSecurityOptions": null,
        "memory": 512,
        "memoryReservation": null,
        "volumesFrom": [],
        "image": "registry.aquasec.com/console:4.2",
        "disableNetworking": null,
        "interactive": null,
        "healthCheck": null,
        "essential": true,
        "hostname": null,
        "extraHosts": null,
        "pseudoTerminal": null,
        "user": null,
        "readonlyRootFilesystem": null,
        "dockerLabels": null,
        "systemControls": null,
        "privileged": null,
        "name": "aqua-server"
    }
]